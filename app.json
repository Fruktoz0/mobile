{
  "expo": {
    "name": "TisztaVaros",
    "slug": "tisztavaros",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/tisztavaros_logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notification/notification_icon.png",
          "color": "#ffffff",
          "defaultChannel": "default",
          "enableBackgroundRemoteNotifications": true
        }
      ]
    ],
    "splash": {
      "image": "./assets/images/tisztavaros_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.fruktoz.tisztavaros",
      "versionCode": 7,
      "googleServicesFile": "./android/app/google-services.json",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "CAMERA",
        "INTERNET",
        "READ_EXTERNAL_STORAGE",
        "READ_MEDIA_IMAGES",
        "RECEIVE_BOOT_COMPLETED"
      ]
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "a6a9bdd2-1326-4ec2-8aa4-2a922b653826"
      }
    },
    "owner": "fruktoz"
  }
}